<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Page7Activity">

    <View
        android:id="@+id/top"
        android:layout_width="405dp"
        android:layout_height="95dp"
        android:background="@drawable/round_button"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/back"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginTop="8dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="@+id/top"
        app:layout_constraintHorizontal_bias="0.009"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back" />

    <ImageView
        android:id="@+id/iv1"
        android:layout_width="275dp"
        android:layout_height="64dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.395"
        app:layout_constraintStart_toEndOf="@+id/back"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/page5up" />

    <ImageView
        android:id="@+id/iv2"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/iv1"
        app:layout_constraintStart_toStartOf="@+id/iv1"
        app:layout_constraintTop_toTopOf="@+id/iv1"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/ic_error_black_24dp"
        tools:ignore="VectorDrawableCompat" />

    <TextView
        android:id="@+id/txv1"
        android:layout_width="157dp"
        android:layout_height="53dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:gravity="center"
        android:text="注\t意\t事\t項"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/iv1"
        app:layout_constraintEnd_toEndOf="@+id/iv1"
        app:layout_constraintTop_toTopOf="@+id/iv1"
        app:layout_constraintVertical_bias="0.454" />

    <ImageButton
        android:id="@+id/ib1"
        android:layout_width="290dp"
        android:layout_height="78dp"
        android:background="@drawable/search"
        app:layout_constraintBottom_toTopOf="@+id/ib2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/top" />

    <TextView
        android:id="@+id/txv2"
        android:layout_width="201dp"
        android:layout_height="53dp"
        android:text="情\t\t境\t\t一"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="171dp"
        app:layout_constraintTop_toTopOf="@+id/ib1"
        app:layout_constraintEnd_toEndOf="@+id/ib1"
        app:layout_constraintStart_toStartOf="@+id/ib1"
        app:layout_constraintBottom_toBottomOf="@+id/ib1"/>

    <ImageButton
        android:id="@+id/ib2"
        android:layout_width="290dp"
        android:layout_height="78dp"
        android:background="@drawable/search"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/ib3"
        app:layout_constraintTop_toBottomOf="@+id/ib1"
        tools:layout_editor_absoluteY="246dp" />

    <TextView
        android:id="@+id/txv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="情\t\t境\t\t二"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintTop_toTopOf="@+id/ib2"
        app:layout_constraintEnd_toEndOf="@+id/ib2"
        app:layout_constraintStart_toStartOf="@+id/ib2"
        app:layout_constraintBottom_toBottomOf="@+id/ib2"/>

    <ImageButton
        android:id="@+id/ib3"
        android:layout_width="290dp"
        android:layout_height="78dp"
        android:background="@drawable/search"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/ib4"
        app:layout_constraintTop_toBottomOf="@+id/ib2" />

    <TextView
        android:id="@+id/txv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="情\t\t境\t\t三"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintTop_toTopOf="@+id/ib3"
        app:layout_constraintEnd_toEndOf="@+id/ib3"
        app:layout_constraintStart_toStartOf="@+id/ib3"
        app:layout_constraintBottom_toBottomOf="@+id/ib3"/>

    <ImageButton
        android:id="@+id/ib4"
        android:layout_width="290dp"
        android:layout_height="78dp"
        android:background="@drawable/search"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ib3"/>

    <TextView
        android:id="@+id/txv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="情\t\t境\t\t四"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintTop_toTopOf="@+id/ib4"
        app:layout_constraintEnd_toEndOf="@+id/ib4"
        app:layout_constraintStart_toStartOf="@+id/ib4"
        app:layout_constraintBottom_toBottomOf="@+id/ib4"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:src="@drawable/ic_priority_high_black_24dp"
        app:backgroundTint="#FD0318"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.971"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.971" />

</androidx.constraintlayout.widget.ConstraintLayout>